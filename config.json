{
  "routes": {
    "404": {
      "html": "/views/404.html",
      "layout": "/views/layout.html"
    },
    "/test": {
      "html": "/views/test.html",
      "layout": "/views/layout.html"
    },
    "/": {
      "html": "/views/home.html",
      "layout": "/views/layout.html"
    },
    "/blog": {
      "html": "/views/blog.html",
      "layout": "/views/layout.html",
      "script": "/js/blog.js"
    },
    "/blog/(?<page>[a-zA-Z0-9_]+)": {
      "html": "/views/blog_page.html",
      "layout": "/views/layout.html",
      "isRegex": true,
      "script": "/js/blog.js"
    },
    "/counter": {
      "html": "/views/counter.html",
      "layout": "/views/layout.html"
    }
  }
}
