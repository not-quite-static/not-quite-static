{
  "routes": {
    "404": {
      "html": "/views/404.html",
      "layout": "/layouts/base.html"
    },
    "/test": {
      "html": "/views/test.html",
      "layout": "/layouts/base.html"
    },
    "/license": {
      "html": "/views/license.html",
      "layout": "/layouts/base.html",
      "script": "/js/blog.js"
    },
    "/": {
      "html": "/views/home.html",
      "layout": "/layouts/base.html",
      "script": "/js/blog.js"
    },
    "/blog": {
      "html": "/views/blog.html",
      "layout": "/layouts/base.html",
      "script": "/js/blog.js"
    },
    "/blog/(?<page>[a-zA-Z0-9_]+)": {
      "html": "/views/blog_page.html",
      "layout": "/layouts/base.html",
      "isRegex": true,
      "script": "/js/blog.js"
    }
  }
}
